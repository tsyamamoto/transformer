<resources>
	<connector-connection-pool
		max-pool-size="250" steady-pool-size="1" name="jms/HelloQCF"
		resource-adapter-name="jmsra"
		connection-definition-name="javax.jms.QueueConnectionFactory"></connector-connection-pool>
	<connector-resource pool-name="jms/HelloQCF"
		jndi-name="jms/HelloQCF"></connector-resource>
	<admin-object-resource res-adapter="jmsra"
		res-type="javax.jms.Queue" jndi-name="jms/InQueue">
		<property name="Name" value="InQueue"></property>
	</admin-object-resource>
	<admin-object-resource res-adapter="jmsra"
		res-type="javax.jms.Queue" jndi-name="jms/OutQueue">
		<property name="Name" value="OutQueue"></property>
	</admin-object-resource>
</resources>
